spring:
  datasource:
    url: jdbc:mysql://127.0.0.1:DB_PORT/DB_NAME
    username: DB_USER
    password: DB_PASS
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    hibernate:
      ddl-auto: update
    properties:
      hibernate:
        show_sql: true
        format_sql: true
        globally_quoted_identifiers: true